{
  "name": "@mfellner/valtio-factory",
  "version": "1.0.0-alpha.9",
  "description": "Create and compose valtio state using the factory pattern.",
  "author": "Maximilian Fellner <mail@mfellner.eu>",
  "repository": "https://github.com/mfellner/valtio-factory",
  "keywords": [
    "valtio",
    "factory",
    "react",
    "state",
    "proxy",
    "store"
  ],
  "license": "MIT",
  "source": "lib/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.mjs",
  "types": "dist/types.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=12.7.0"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "start": "parcel --dist-dir build example/index.html",
    "build": "parcel build",
    "clean": "rm -rf dist/ .parcel-cache/",
    "test": "jest",
    "typecheck": "yarn tsc -p tsconfig.json --noEmit",
    "release": "release-it",
    "prepare": "husky install",
    "reinstall": "rm -rf node_modules yarn.lock && yarn install"
  },
  "lint-staged": {
    ".ts": "eslint --fix"
  },
  "peerDependencies": {
    "valtio": ">=1.2.11"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@parcel/packager-ts": "2.6.2",
    "@parcel/transformer-typescript-types": "2.6.2",
    "@release-it/conventional-changelog": "5.0.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@types/jest": "28.1.5",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "eslint": "8.19.0",
    "eslint-plugin-valtio": "0.4.4",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "jest-runner-eslint": "1.0.1",
    "lint-staged": "13.0.3",
    "parcel": "2.6.2",
    "process": "^0.11.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "release-it": "15.1.1",
    "ts-jest": "28.0.5",
    "typescript": "4.7.4",
    "valtio": "1.6.2"
  }
}
